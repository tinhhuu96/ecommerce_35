<tr>
  <td><%= order.id %></td>
  <td><%= order.name %></td>
  <td><%= order.email %></td>
  <td><%= order.created_at %></td>
  <td><%= order.address %></td>
  <td><%= order.subtotal %> $</td>
  <td class="text_<%= order.status %>"><%= order.status %></td>
  <td><%= link_to t(".view_order"), manager_order_path(order) %></td>
  <td><%= link_to t(".delete"), manager_order_path(order),method: :delete %></td>
</tr>
